{
  "extends": "./tsconfig.app.json",
  "compilerOptions": {
    "skipLibCheck": true,
    "noEmit": true,
    "composite": false,
    "noImplicitAny": false,
    "allowJs": true,
    "checkJs": false,
    "suppressImplicitAnyIndexErrors": true,
    "skipDefaultLibCheck": true
  },
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue"
  ]
} 